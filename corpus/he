# ל כ ל # ו - # 0 < / m a t h > . # מ כ א ן # מ ת ק ב ל # ל ד ו ג מ ה # ה ש ו ו י ו ן # ^ { - 1 } # + 1 < / m a t h > , # ו א ת # ה ב י ט ו י # א פ ש ר # ל פ ש ט # ל ב י ט ו י # פ ו ל י נ ו מ י ם # א י - פ ר י ק י ם # א ם # ה ו א # פ ו ל י נ ו ם # א י - פ ר י ק # מ מ ע ל ה # מ ע ל # ה ש ד ה # \ m a t h b b { f } _ q < / m a t h > , # א ז # ה ו א # מ ח ל ק # א ת # f ( x ) = x ^ { q ^ n } - x < / m a t h > ; # מ צ ד # ש נ י , # ה פ י ר ו ק # ל ג ו ר מ י ם # א י - פ ר י ק י ם # כ ו ל ל # א ת # כ ל # ה פ ו ל י נ ו מ י ם # ה א י - פ ר י ק י ם # ה מ ת ו ק נ י ם , # מ מ ע ל ה # ה מ ח ל ק ת # א ת # א ם # נ ס מ ן # ב - # א ת # מ ס פ ר # ה פ ו ל י נ ו מ י ם # ה מ ת ו ק נ י ם # ה א י - פ ר י ק י ם # מ מ ע ל ה # מ ת ק ב ל ת # מ כ א ן # ה נ ו ס ח ה # \ s u m _ { d | n } d a _ d ( q ) = q ^ n < / m a t h > . # א פ ש ר # ל ה פ ו ך # א ת # ה ס ד ר # ב ע ז ר ת # ה ה י פ ו ך # a _ n ( q ) # \ f r a c { 1 } { n } \ s u m _ { d | n } \ m u ( n / d ) q ^ d < / m a t h > . # ה ג ו ר ם # ה מ ש מ ע ו ת י # ב י ו ת ר # ב ס כ ו ם # ז ה # ה ו א # ה ג ו ר ם # ה מ ת א י ם # ל - # d = n , # ו ל כ ן # a _ n ( q ) # \ f r a c { 1 } { n } q ^ n < / m a t h > . # l i c h a a m # ( n e d ) # e i n d i g
